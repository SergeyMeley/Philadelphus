<UserControl x:Class="Philadelphus.Presentation.Wpf.UI.Views.Controls.RepositoryOpen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Philadelphus.Presentation.Wpf.UI.Views.Controls"
             xmlns:ctrls="clr-namespace:Philadelphus.Presentation.Wpf.UI.Views.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TabControl>
            <TabItem Header="Избранные и недавние">
                <ctrls:FavoriteAndLastRepositoryHeaders></ctrls:FavoriteAndLastRepositoryHeaders>
            </TabItem>
            <TabItem Header="Доступные">
                <ctrls:AvailableRepositoryHeaders></ctrls:AvailableRepositoryHeaders>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
