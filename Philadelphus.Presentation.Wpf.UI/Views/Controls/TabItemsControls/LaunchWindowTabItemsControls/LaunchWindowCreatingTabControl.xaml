<UserControl x:Class="Philadelphus.Presentation.Wpf.UI.Views.Controls.TabItemsControls.LaunchWindowTabItemsControls.LaunchWindowCreatingTabControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Philadelphus.Presentation.Wpf.UI.Views.Controls.TabItemsControls.LaunchWindowTabItemsControls"
             xmlns:ctrls="clr-namespace:Philadelphus.Presentation.Wpf.UI.Views.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <ctrls:RepositoryCreation DataContext="{Binding RepositoryCreationVM}" Grid.Row="0"></ctrls:RepositoryCreation>
        <GroupBox Grid.Row="1" Header="Выбор шаблона">
            <ListBox>
                <ListBoxItem Content="Пустой репозиторий" Height="50"></ListBoxItem>
            </ListBox>
        </GroupBox>
    </Grid>
</UserControl>
